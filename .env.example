PRIVATE_KEY=           # EOA private key (hex, with or without 0x prefix)
PIMLICO_API_KEY=       # Pimlico bundler + paymaster API key
TELEGRAM_BOT_TOKEN=    # Telegraf bot token from BotFather
TELEGRAM_BOT_USERNAME= # Bot @username without @, e.g. defai_bharat_bot
GROQ_API_KEY=          # Groq API key (Llama 3.3 70B for intent parsing)
TINYFISH_API_KEY=      # TinyFish web-scraping agent API key
BSC_TESTNET_RPC=       # Optional: custom BSC Testnet RPC URL
JWT_SECRET=            # JWT signing secret for dashboard API auth
ENCRYPTION_KEY=        # Server-side key for encrypting private keys (default: defai-dev-default)
DEFAI_USER_ID=         # Optional: resume MCP session as existing user
MCP_TRANSPORT=         # "stdio" (default) or "sse"
